{
  "list.height": 15,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.floating.border.enable": false,
  "explorer.file.showHiddenFiles": true,
  "eslint.autoFixOnSave": true,
  "prettier.eslintIntegration": true,
  "coc.source.word.filetypes": ["markdown", "text"],
  "tabnine.priority": 100,
  "tsserver.trace.server": "verbose",
  "tsserver.enableJavascript": true,
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.jumpCommand": "drop",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "diagnostic.enable": true,
  "diagnostic-languageserver.enable": true,
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "sh": "shellcheck"
  },
  "diagnostic-languageserver.formatters": {
    "clang-format": {
      "command": "clang-format"
    },
    "vint": {
      "command": "vint"
    },
    "gofmt": {
      "command": "gofmt",
      "args": ["-w", "-"]
    }
  },
  "diagnostic-languageserver.formatFiletypes": {
    "vim": "vint",
    "lua": "lua-format",
    "sh": "shfmt",
    "c": "clang-format",
    "cpp": "clang-format",
    "go": "gofmt"
  },
  "suggest.completionItemKindLabels": {
    "keyword": " [keyword]",
    "variable": "識[variable]",
    "value": "\uf89f [value]",
    "operator": "\u03a8 [operator]",
    "function": " [function]",
    "reference": "\ufa46 [reference]",
    "constant": "識[constant]",
    "method": " [method]",
    "struct": "\ufb44 [struct]",
    "class": "\uf0e8 [class]",
    "interface": "\uf417 [interface]",
    "text": "\ue612 [text]",
    "enum": "\uf435 [enum]",
    "enumMember": "\uf02b [enumMember]",
    "module": "\uf40d [module]",
    "color": "\ue22b [color]",
    "property": " [property]",
    "field": "\uf9be[field]",
    "unit": "\uf475 [unit]",
    "event": "\ufacd [event]",
    "file": "\uf723 [file]",
    "folder": "\uf114 [folder]",
    "snippet": "\ue60b [snippet]",
    "typeParameter": "\uf728 [type]",
    "default": "\uf29c [default]"
  },
  "diagnostic.format": "%message [%source]",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": " ",
  "diagnostic.hintSign": " ",
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "explorer.git.icon.status.modified": "✹",
  "explorer.git.icon.status.added": "✚",
  "explorer.git.icon.status.unmerged": "═",
  "explorer.git.icon.status.renamed": "➜",
  "explorer.git.icon.status.deleted": "✖",
  "explorer.git.icon.status.untracked": "",
  "explorer.git.icon.status.copied": "",
  "explorer.git.icon.status.mixed": "✭",
  "explorer.git.icon.status.unmodified": " ",
  "explorer.git.icon.status.ignored": "﯏",
  "git.branchCharacter": "",
  "git.changedDecorator": "✹ ",
  "git.stagedDecorator": "● ",
  "git.conflictedDecorator": "✘",
  "git.untrackedDecorator": " "
}
